<?xml version="1.0"?>
<package format="2">
  <name>navigation_control</name>
  <version>1.0.0</version>
  <description>
    Package containing high level controller for SubT solution.
    This includes the ability to generate an exploration graph via the 
    detection of new frontiers, and the state machine to control move_base.
  </description>

  <author email="ashton@openrobotics.org">Ashton Larkin</author>
  <author email="gonzalodepedro@ekumenlabs.com">Gonzalo de Pedro</author>
  <author email="jtlorente@ekumenlabs.com">Jose Tomas Lorente</author>
  <author email="michael@openrobotics.org">Michael Carroll</author>

  <maintainer email="ashton@openrobotics.org">Ashton Larkin</maintainer>

  <license>Apache License 2.0</license>

  <buildtool_depend>catkin</buildtool_depend>

  <depend>actionlib</depend>
  <depend>base_local_planner</depend>
  <depend>costmap_2d</depend>
  <depend>geometry_msgs</depend>
  <depend>move_base_msgs</depend>
  <depend>roscpp</depend>
  <depend>visualization_msgs</depend>
  <depend>ignition-msgs4</depend>

  <build_depend>protobuf-dev</build_depend>
  <build_export_depend>protobuf-dev</build_export_depend>

  <exec_depend>protobuf</exec_depend>
</package>
