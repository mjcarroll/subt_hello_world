<?xml version="1.0"?>
<package format="2">
  <name>z_axis_flattener</name>
  <version>1.0.0</version>
  <description>
    Creates a flattened tf frame for move_base to work on a 3D environment.
  </description>

  <author email="jtlorente@ekumenlabs.com">Jose Tomas Lorente</author>

  <maintainer email="jtlorente@ekumenlabs.com">Jose Tomas Lorente</maintainer>

  <license>Apache License 2.0</license>

  <buildtool_depend>catkin</buildtool_depend>

  <depend>roslaunch</depend>
  <depend>roscpp</depend>
  <depend>geometry_msgs</depend>
  <depend>tf2</depend>
  <depend>tf2_ros</depend>
  <depend>tf2_geometry_msgs</depend>

</package>
