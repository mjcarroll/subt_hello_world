<?xml version="1.0"?>
<package format="2">
  <name>artifact_localization</name>
  <version>1.0.0</version>
  <description>
  The artifact_localization package. This package determines where a SubT artifact is with respect to the artifact_origin.
  This package uses the darknet_ros repository to help with object detection, and then uses the Point Cloud library to map the
  bounding box from darknet_ros to a pose relative to the artifact_origin.
  </description>

  <author email="ashton@openrobotics.org">Ashton Larkin</author>
  <author email="jtlorente@ekumenlabs.com">Jose Tomas Lorente</author>

  <maintainer email="ashton@openrobotics.org">Ashton Larkin</maintainer>
  <maintainer email="jtlorente@ekumenlabs.com">Jose Tomas Lorente</maintainer>

  <license>Apache License 2.0</license>

  <buildtool_depend>catkin</buildtool_depend>

  <build_depend>pcl_conversions</build_depend>
  <build_depend>pcl_ros</build_depend>
  <build_depend>roscpp</build_depend>
  <build_depend>sensor_msgs</build_depend>

  <build_export_depend>pcl_conversions</build_export_depend>
  <build_export_depend>pcl_ros</build_export_depend>
  <build_export_depend>roscpp</build_export_depend>
  <build_export_depend>sensor_msgs</build_export_depend>

  <exec_depend>pcl_conversions</exec_depend>
  <exec_depend>pcl_ros</exec_depend>
  <exec_depend>roscpp</exec_depend>
  <exec_depend>sensor_msgs</exec_depend>

  <depend>artifact_localization_msgs</depend>
  <depend>darknet_ros_msgs</depend>
  <depend>tf2</depend>
  <depend>tf2_ros</depend>
  <depend>subt_msgs</depend>
  <depend>subt_ign</depend>
  <depend>std_msgs</depend>
  <depend>visualization_msgs</depend>
</package>
